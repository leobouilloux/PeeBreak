    pod 'RealmSwift'
    pod 'RxAlamofire'
    pod 'RxCocoa', '~> 4.0'
    pod 'RxRealm'
    pod 'RxSwift', '~> 4.0'
    pod 'RxTheme', '~> 3.0'
    pod 'SwiftGen', '~> 6.0'
    pod 'SwiftLint'
    pod 'SwiftyJSON', '~> 4.0'
end

target 'Pee break' do
    pods
end
